<h1><%= @user.alias %></h1>


<h3>Published Articles</h3>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Summary</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @articles.each do |article| %>
      <tr>
        <td><%= article.title %></td>
        <td><%= article.summary %></td>
      </tr>
    <% end %>
  </tbody>
</table>